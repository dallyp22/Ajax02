# ===========================================
# PRODUCTION FRONTEND CONFIGURATION
# ===========================================
# Configure these in Vercel Dashboard

# Application Settings
VITE_ENVIRONMENT=production
VITE_APP_NAME=AI Rent Optimizer
VITE_DEMO_MODE=false

# API Configuration (REQUIRED - update with your Railway backend URL)
VITE_API_URL=https://your-backend.railway.app/api/v1

# Auth0 Configuration (REQUIRED)
VITE_AUTH0_DOMAIN=your-auth0-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-auth0-client-id
VITE_AUTH0_AUDIENCE=https://your-api-identifier
VITE_AUTH0_REDIRECT_URI=https://your-app.vercel.app/callback

# Authentication Bypass (set to false for production)
VITE_BYPASS_AUTH=false